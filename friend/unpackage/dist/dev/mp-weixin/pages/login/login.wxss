
.login{
		-webkit-box-flex: 1;
		-webkit-flex: 1;
		    -ms-flex: 1;
		        flex: 1;
		padding:0px;
		border: 1px solid blue;
}
.title-nav{
		display: -webkit-box;
		display: -webkit-flex;
		display: -ms-flexbox;
		display: flex;
		-webkit-box-orient: horizontal;
		-webkit-box-direction: normal;
		-webkit-flex-direction: row;
		    -ms-flex-direction: row;
		        flex-direction: row;
		-webkit-box-pack:end;
		-webkit-justify-content:flex-end;
		    -ms-flex-pack:end;
		        justify-content:flex-end;
}
.title-nav .text{
		-webkit-align-self:flex-end;
		    -ms-flex-item-align:end;
		        align-self:flex-end;
		margin-bottom:60rpx;
		font-size: 32px;
		font-weight: bold;
		-webkit-box-flex:1;
		-webkit-flex-grow:1;
		    -ms-flex-positive:1;
		        flex-grow:1;
		text-align: left;
		padding-left:60rpx;
}
.title-nav .image-content{
		width: 55%;
}
.action-row {
        display: -webkit-box;
        display: -webkit-flex;
        display: -ms-flexbox;
        display: flex;
        -webkit-box-orient: horizontal;
        -webkit-box-direction: normal;
        -webkit-flex-direction: row;
            -ms-flex-direction: row;
                flex-direction: row;
        -webkit-box-pack: center;
        -webkit-justify-content: center;
            -ms-flex-pack: center;
                justify-content: center;
		color: #7558ff;
}
.action-row navigator {
        color: #007aff;
        padding: 0 20rpx;
}
.statement{
		display: -webkit-box;
		display: -webkit-flex;
		display: -ms-flexbox;
		display: flex;
		margin-top: 30rpx;
		-webkit-box-pack: center;
		-webkit-justify-content: center;
		    -ms-flex-pack: center;
		        justify-content: center;
}
.statement .link{
		text-decoration: underline;
}
.split-line{
		color:#c8c7cc;
}
.oauth-login{
		width: 100%;
		-webkit-align-self: flex-end;
		    -ms-flex-item-align: end;
		        align-self: flex-end;
}
.other-login{
		position: relative;
		height: 16px;
		margin: 0 40rpx;
}
.other-login .text {
		color:#8F8F94;
		position: absolute;
		width:280rpx;
		left: 0;
		right: 0;
		top: 0;
		background-color: #FFFFFF;
		margin: auto;
		text-align: center;
}
.other-login .line{
		position: absolute;
		width:100%;
		height: 1rpx;
		left: 0;
		top: 6px;
		content: '';
		-webkit-transform: scaleY(.5);
		-ms-transform: scaleY(.5);
		    transform: scaleY(.5);
		background-color: #c8c7cc;
}
.oauth-row {
        display: -webkit-box;
        display: -webkit-flex;
        display: -ms-flexbox;
        display: flex;
        -webkit-box-orient: horizontal;
        -webkit-box-direction: normal;
        -webkit-flex-direction: row;
            -ms-flex-direction: row;
                flex-direction: row;
        -webkit-box-pack: center;
        -webkit-justify-content: center;
            -ms-flex-pack: center;
                justify-content: center;   
        width: 100%;
		padding-top:50rpx;
		padding-bottom:50rpx;
}
.oauth-image {
        width: 100rpx;
        height: 100rpx;
        border-radius: 100rpx;
        margin: 0 40rpx;
		background-color: #ffffff;
		-webkit-box-shadow:5rpx 5rpx 30rpx 5rpx #efecf4;
		        box-shadow:5rpx 5rpx 30rpx 5rpx #efecf4;
}
.oauth-image image {
        width: 60rpx;
        height: 60rpx;
        margin: 20rpx;
}
.oauth-button{
		width: 60rpx;
		height: 60rpx;
		margin: 20rpx;
		background: url(data:image/png;base64,) center no-repeat;
		background-size: 100%;
		border: 0;
}
	/* 去掉自带边框 */
.oauth-button::after{ border: none;} 

